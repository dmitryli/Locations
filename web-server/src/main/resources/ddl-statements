CREATE TABLE TENANTS (ID INT NOT NULL AUTO_INCREMENT PRIMARY KEY, NAME VARCHAR(128));

CREATE TABLE LOCATIONS(ID INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
ADDRESS VARCHAR(128), CITY VARCHAR(64), STATE VARCHAR(64), COUNTRY VARCHAR(64), ZIP VARCHAR(64));

CREATE TABLE TENANTS_ADMINS(ID INT NOT NULL AUTO_INCREMENT PRIMARY KEY, TENANT_ID INT, 
NAME VARCHAR(128), LAST_NAME VARCHAR(128), PASSWORD VARCHAR(256),
EMAIL VARCHAR(128), STATUS INT, CREATE_ADMINS INT, DELETE_ADMINS INT, CREATE_LOCATIONS INT, 
DELETE_LOCATIONS INT, CREATE_USERS INT, DELETE_USERS INT, CHANGE_USER_STATUS INT, CHANGE_GUEST_STATUS INT);

CREATE TABLE TENANTS_ADMINS_LOCATIONS(ID INT NOT NULL AUTO_INCREMENT PRIMARY KEY, TENANT_ID INT,
ADMIN_ID INT, LOCATION_ID INT, ADMIN_LOCATION_STATUS INT);

CREATE TABLE TENANTS_USERS(ID INT NOT NULL AUTO_INCREMENT PRIMARY KEY, TENANT_ID INT, 
NAME VARCHAR(128), LAST_NAME VARCHAR(128), EMAIL VARCHAR(128), PASSWORD VARCHAR(256), STATUS INT); 

CREATE TABLE TENANTS_USERS_LOCATIONS(ID INT NOT NULL AUTO_INCREMENT PRIMARY KEY, TENANT_ID INT,
USER_ID INT, LOCATION_ID INT, START_TIME TIMESTAMP DEFAULT CURRENT_TIMESTAMP, END_TIME TIMESTAMP DEFAULT CURRENT_TIMESTAMP);


CREATE TABLE TENANTS_GUESTS(ID INT NOT NULL AUTO_INCREMENT PRIMARY KEY, TENANT_ID INT, 
USER_ID INT,NAME VARCHAR(128), LAST_NAME VARCHAR(128), EMAIL VARCHAR(128), ORGANIZATION VARCHAR(128),
START_TIME TIMESTAMP DEFAULT CURRENT_TIMESTAMP, END_TIME TIMESTAMP DEFAULT CURRENT_TIMESTAMP)
  